{
  "name": "test-scope",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "validate:eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "validate:stylelint": "stylelint --fix \"./src/**/*.scss\"",
    "validate:deps": "unimported",
    "storybook:build": "build-storybook -s public",
    "storybook:start": "start-storybook -p 6006 --no-manager-cache",
    "app:prepare": "node ./scripts/prepare.js && tsc --noEmit",
    "app:watchers": "node ./scripts/watch.js",
    "app:dev": "yarn app:prepare && concurrently \"yarn app:watchers\" \"next dev\" --names \"watchers,app\"",
    "app:build": "yarn app:prepare && next build",
    "app:start": "yarn app:prepare && next start",
    "postinstall": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn validate:eslint"
    ],
    "src/**/*.scss": [
      "yarn validate:stylelint"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "effector": "^22.3.0",
    "effector-http-api": "^0.1.1",
    "effector-react": "^22.1.3",
    "effector-view": "^2.0.10",
    "next": "12.1.6",
    "next-seo": "^5.4.0",
    "next-translate": "^1.4.0",
    "nextjs-effector": "^0.2.0",
    "patronum": "^1.8.2",
    "react": "18.1.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@geometricpanda/storybook-addon-badges": "^0.2.1",
    "@next/eslint-plugin-next": "^12.1.6",
    "@storybook/addon-a11y": "^6.5.6",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",

{
  "name": "next-based-template",
  "private": true,
  "scripts": {
    "dev": "tsc --noEmit && next dev",
    "build": "tsc --noEmit && next build",
    "start": "next start",
    "docker": "next start",
    "lint": "next lint --fix",
    "stylelint": "stylelint --fix \"./src/**/*.scss\"",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },

{
  "name": "next-based-template",
  "private": true,
  "scripts": {
    "dev": "tsc --noEmit && next dev",
    "build": "tsc --noEmit && next build",
    "start": "next start",
    "docker": "next start",
    "lint": "next lint",
    "stylelint": "stylelint --fix \"./src/**/*.scss\" && prettier --write \"./src/**/*.scss\"",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "plugins": [
      "commitlint-plugin-jira-rules"
    ],
    "extends": [
      "jira"
    ],
    "rules": {
      "jira-task-id-max-length": [
        2,
        "always",
        12
      ]
    }
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.5.2",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "cookies": "^0.8.0",
    "effector": "^22.2.0",
    "effector-next": "^1.3.2",
    "effector-react": "^22.0.6",
    "formik": "^2.2.9",
    "include-media": "^1.4.10",
    "next": "12.0.7",
    "next-i18next": "^10.5.0",
    "object-path": "^0.11.8",
    "react": "17.0.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.16.6",
    "sass": "^1.45.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@geometricpanda/storybook-addon-badges": "^0.2.1",
    "@storybook/addon-a11y": "^6.4.21",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@types/cookies": "^0.7.7",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.2.4",
    "case": "^1.6.3",
    "commitlint": "^11.0.0",
    "commitlint-config-jira": "^1.4.1",
    "commitlint-plugin-jira-rules": "^1.4.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-unused-imports": "^1.0.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "prettier": "^2.1.2",
    "storybook-addon-breakpoints": "^1.0.0",
    "storybook-addon-code-preview": "^0.0.9",
    "storybook-addon-designs": "^6.2.1",
    "storybook-addon-measure-viewport": "^0.2.1",
    "storybook-addon-next": "^1.6.2",
    "storybook-addon-paddings": "^4.3.0",
    "storybook-addon-pseudo-states": "^1.9.0",
    "storybook-color-picker": "^2.3.1",
    "storybook-dark-mode": "^1.0.9",
    "stylelint": "^13.7.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "typescript": "4.5.4",
    "webpack": "^5.72.0",
    "webpack-merge": "^5.8.0"
  }
}
